#!/usr/bin/env bash

#homebrew
export PATH="/usr/local/bin:$PATH"
export PATH="/usr/local/sbin:$PATH"
#composer
export PATH="$PATH:~/.composer/vendor/bin"
#gnu utils
export PATH="$PATH:(brew --prefix coreutils)/libexec/gnubin"
#brew cask install path
export HOMEBREW_CASK_OPTS="--appdir=/Applications"
#go $ brew info go
# https://gist.github.com/vsouza/77e6b20520d07652ed7d
export GOPATH="$HOME/golang"
export GOROOT="/usr/local/opt/go/libexec"
export PATH="$PATH:$GOPATH/bin"
export PATH="$PATH:$GOROOT/bin"
# homebrew cask contrib
export PATH="$PATH:/usr/local/Library/Taps/caskroom/homebrew-cask/developer/bin"
#brewed python
export PATH="$PATH:$(brew --prefix python)/libexec/bin"
#mopidy
export PYTHONPATH="$(brew --prefix)/lib/python2.7/site-packages:$PYTHONPATH"
#yarn
export PATH="$PATH:$HOME/.yarn/bin"
#brewed bison
export PATH="$PATH:/usr/local/opt/bison/bin"
#brewed curl
export PATH="$PATH:/usr/local/opt/curl/bin"
#other brewed formulas
export PATH="$PATH:/usr/local/opt/dirmngr/bin"
export PATH="/usr/local/opt/curl/bin:$PATH"
export PATH="/usr/local/opt/icu4c/bin:$PATH"
export PATH="/usr/local/opt/icu4c/sbin:$PATH"
export PATH="/usr/local/opt/libxml2/bin:$PATH"
export PATH="/usr/local/opt/qt/bin:$PATH"
export PATH="/usr/local/opt/unzip/bin:$PATH"

export PATH="/Applications/Redis.app/Contents/Resources/Vendor/redis/bin:$PATH"

export PATH="/usr/local/opt/nss/bin:$PATH"

#pyenv  - https://github.com/pyenv/pyenv
export PYENV_ROOT="$HOME/.pyenv"
export PATH="$PYENV_ROOT/bin:$PATH"
